<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.zensar.training.bean.Doctor" table="doctors_50">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="name" />
		<property name="fees" />
		<property name="gender" />
		<property name="practicingSince" />

	</class>

</hibernate-mapping>